    		"height": 200,
    		"width": 300,
			"dataSource": "sampleChartData",
			"defaultOptions": {"valueMemberPath": "Pop2008", "labelMemberPath": "CountryName", "labelsPosition": "bestFit", "allowSliceExplosion": false,  "allowSliceSelection": false } 
