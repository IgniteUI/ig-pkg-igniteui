{
    "name": "igniteui",
    "toolboxTitle": "Ignite UI",
    "scripts": [
        {
            "url": "http://code.jquery.com/jquery-1.10.2.min.js",
            "lazyLoad": false
        },
        {
            "url": "http://code.jquery.com/ui/1.10.4/jquery-ui.min.js",
            "lazyLoad": false
        },
        {
            "url": "/js/infragistics.core.js",
            "lazyLoad": false,
            "id": "core"
        },
        {
            "url": "/js/infragistics.lob.js",
            "lazyLoad": false,
            "id": "lob"
        },
        {
            "url": "js/infragistics.dv.js",
            "lazyLoad": true,
            "id": "dv"
        }
    ],
    "stylesheets": [
        "css/themes/infragistics/infragistics.theme.css",
        "css/structure/infragistics.css"
    ],
    "iconsPath": "icons",
    "iconSpriteCssFile": "ignite-icons.css",
    "iconSpriteCssPrefix": "ignite-icons", 
    "configPath": "config",
    "pluginsPath": "plugins",
    "autocompletePath": "autocomplete", 
    "defaultComponentPlugin": "_default-component-plugin",
    "codeTemplatesPath": "templates",
    "requiresInitialization": true,
    "components": {
    	"grid": {
    		"id": "grid",
    		"toolboxTitle": "Grid",
    		"componentPlugin": "grid-component-plugin", 
    		"icon": "grid.png",
    		"dragDropIcon": "grid-dragdrop.png",
    		"category": "lists",
    		"visual": true, 
    		"framework": false,
    		"height": 300,
    		"width": 300,
    		"dataSource": "sampleData1"
    	},
    	"hierarchicalGrid": {
    		"id": "hierarchicalGrid",
    		"toolboxTitle": "Hierarchical Grid",
    		"icon": "hierarchicalGrid.png",
			"componentPlugin": "hierarchical-grid-component-plugin",
    		"dragDropIcon": "hierarchicalgrid-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 300,
    		"height": 300,
    		"dataSource": "sampleHierarchicalData1",
    		"defaultOptions": {"autoGenerateLayouts": true}
    	},
    	"dataChart": {
    		"id": "dataChart",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Chart",
    		"icon": "chart.png",
    		"dragDropIcon": "chart-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 200,
    		"width": 500,
    		"dataSource": "sampleChartData",
			"defaultOptions" : {
				"axes": [
                    {
                        "name": "NameAxis",
                        "type": "categoryX",
                        "title": "Population",
                        "label": "CountryName"
                    },
					{
                        "name": "PopulationAxis",
                        "type": "numericY",
						"minimumValue": 0
                    }
                ],
				"series": [
                    {
                        "name": "Pop2008",
                        "type": "column",
                        "xAxis": "NameAxis",
                        "yAxis": "PopulationAxis",
                        "valueMemberPath": "Pop2008"
                    }
                ]
			}
    	},
		"pieChart": {
    		"id": "pieChart",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Pie Chart",
    		"icon": "pie-chart.png",
    		"dragDropIcon": "pie-chart-dragdrop.png",
			"componentPlugin": "piechart-component-plugin", 
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 200,
    		"width": 200,
			"dataSource": "sampleChartData",
			"defaultOptions": {"valueMemberPath": "Pop2008", "labelMemberPath": "CountryName", "labelsPosition": "bestFit" } 
		},	
		"sparkline": {
    		"id": "sparkline",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Sparkline",
    		"icon": "sparkline.png",
    		"dragDropIcon": "sparkline-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 100,
    		"width": 300,
			"dataSource": "sampleSparklineData",
			"defaultOptions": {"valueMemberPath": "ExtendedPrice" } 
		},	
		"videoPlayer": {
    		"id": "videoPlayer",
    		"toolboxTitle": "Video Player",
    		"icon": "videoplayer.png",
    		"dragDropIcon": "videoplayer-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 300,
    		"width": 700,
			"defaultOptions": { "sources": [
                    "http://dl.infragistics.com/pg/2011-1/web/shared/videoplayer/videos/Infragistics_Presentation_lowRes_1.h264.mp4",
                    "http://dl.infragistics.com/pg/2011-1/web/shared/videoplayer/videos/Infragistics_Presentation_lowRes_1.webmvp8.webm",
                    "http://dl.infragistics.com/pg/2011-1/web/shared/videoplayer/videos/Infragistics_Presentation_lowRes_1.theora.ogv"
                ] 
			}
		},	
		"bulletGraph": {
    		"id": "bulletGraph",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Bulletgraph",
    		"icon": "bullet-graph.png",
    		"dragDropIcon": "bullet-graph-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 100,
    		"width": 300,
			"defaultOptions": { 
				"minimumValue": 0, 
                "maximumValue": 25,
                "interval": 5,
                "value": 17,
                "targetValue": 21
			}
		},
		"doughnutChart": {
    		"id": "doughnutChart",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Doughnut Chart",
    		"icon": "doughnut-chart.png",
    		"dragDropIcon": "doughnut-chart-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 300,
    		"width": 300,
			
			"defaultOptions": {
				"series":
					[{
						"name": "Pop1990",
						"labelMemberPath": "CountryName",
						"valueMemberPath": "Pop1990",
						"dataSource": "sampleDoughnutChartData",
						"labelsPosition": "bestFit"
					}]
			} 
		},
		"funnelChart": {
    		"id": "funnelChart",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Funnel Chart",
    		"icon": "funnel-chart.png",
    		"dragDropIcon": "funnel-chart-dragdrop.png",			
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 450,
    		"width": 325,
			"dataSource": "sampleFunnelChartData",
			"defaultOptions": {"valueMemberPath": "Budget", "innerLabelMemberPath": "Budget", "innerLabelVisibility": "visible", "outerLabelMemberPath": "Department",
                "outerLabelVisibility": "visible" } 
		},			
    	"combo": {
    		"id": "combo",
    		"toolboxTitle": "Combo",
    		"icon": "combo.png",
    		"dragDropIcon": "combo-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 200,
    		"height":30,
    		"defaultOptions": {"dropDownAsChild": true},
    		"dataSource": "sampleListData1"
    	},
    	"dataSource": {
    		"id": "dataSource",
            "componentPlugin": "datasource-component-plugin", 
    		"toolboxTitle": "Data Source",
    		"icon": "datasource.png",
            "dragDropIcon": "datasource-dragdrop.png",
    		"classId": "$.ig.IgDataSource",
    		"category": "datasources",
    		"visual": false,
    		"framework": true
    	},
    	"dialog": {
    		"id": "dialog",
    		"toolboxTitle": "Dialog Window",
    		"icon": "dialogwindow.png",
    		"dragDropIcon": "dialogwindow-dragdrop.png",
