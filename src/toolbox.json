    	"grid": {
    		"id": "grid",
    		"toolboxTitle": "Grid",
    		"componentPlugin": "grid-component-plugin", 
    		"icon": "grid.png",
			"dataBoundable": true,
    		"dragDropIcon": "grid-dragdrop.png",
    		"category": "lists",
    		"visual": true, 
    		"framework": false,
			"width": "100%",
			"height": 450,
    		"dataSource": "defaultGridData1",
			"defaultOptions": {
				"renderCheckboxes": true,
				"autoGenerateColumns": false,
				"columns": [
					{
						"key": "active",
						"headerText": "Active",
						"dataType": "bool",
						"width": "7%"
					},
					{
						"key": "registered",
						"headerText": "Registered",
						"dataType": "date",
						"format": "d MMM yyyy",
						"width": "13%"
					},
					{
						"key": "name",
						"headerText": "Name",
						"width": "20%"
					},
					{
						"key": "company",
						"headerText": "Company",
						"width": "15%"
					},
					{
						"key": "phone",
						"headerText": "Phone",
						"width": "15%"
					},
					{
						"key": "email",
						"headerText": "Email",
						"template": "<a href='mailto:${email}'>${email}</a>",
						"width": "30%"
					}
				],
				"features": [
					{
						"name": "Filtering"
					},
					{
						"name": "Paging",
						"showPageSizeDropDown": false
					},
					{
						"name": "Sorting"
					},
					{
						"name": "Selection"
					}
				]
			}
    	},
    	"hierarchicalGrid": {
    		"id": "hierarchicalGrid",
    		"toolboxTitle": "Hierarchical Grid",
    		"icon": "hierarchicalGrid.png",
			"dataBoundable": true,
			"componentPlugin": "hierarchical-grid-component-plugin",
    		"dragDropIcon": "hierarchicalgrid-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 300,
    		"height": 300,
    		"dataSource": "sampleHierarchicalData1",
    		"defaultOptions": {"autoGenerateLayouts": true, "features": [], "columnLayouts": []}
    	},
    	"dataChart": {
    		"id": "dataChart",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Chart",
			"componentPlugin": "charts-component-plugin",
			"dataBoundable": true,
    		"icon": "chart.png",
    		"dragDropIcon": "chart-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"dataSource": "sampleChartData",
			"defaultOptions": {
				"width": "100%",
				"height": 300,
				"axes": [
                    {
                        "name": "NameAxis",
                        "type": "categoryX",
                        "title": "Population",
                        "label": "CountryName"
                    },
					{
                        "name": "PopulationAxis",
                        "type": "numericY",
						"minimumValue": 0
                    }
                ],
				"series": [
                    {
                        "name": "Pop2008",
                        "type": "column",
                        "xAxis": "NameAxis",
                        "yAxis": "PopulationAxis",
                        "valueMemberPath": "Pop2008",
                        "isHighlightingEnabled": true,
                        "isTransitionInEnabled": true,
                        "transitionInMode": "accordionFromValueAxisMinimum",
                        "showTooltip": true,
                        "tooltipTemplate": "<div>${item.CountryName}: <strong>${item.Pop2008} Million</strong></div>"
                    }
                ]
			}
    	},
