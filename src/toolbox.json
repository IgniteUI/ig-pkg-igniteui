    		"icon": "fileupload.png",
    		"dragDropIcon": "fileupload-dragdrop.png",
    		"category": "editors",
    		"visual": true,
			"width": 200,
    		"framework": false
    	},
    	"radialGauge": {
    		"id": "radialGauge",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Radial Gauge",
    		"icon": "gauge.png",
    		"dragDropIcon": "radial-gauge-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"width": 150,
			"height": 150,
    		"defaultOptions": {
                "value": "20"
    		}
    	},
		"linearGauge": {
    		"id": "linearGauge",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Linear Gauge",
    		"icon": "linear-gauge.png",
    		"dragDropIcon": "linear-gauge-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"width": 500,
			"height": 80,
    		"defaultOptions": {
                "value": -273,
                "minimumValue": -275,
                "maximumValue": 0,
                "ranges": [
                    { "startValue": -275, "endValue": -225, "name": "range1" },
                    { "startValue": -225, "endValue": -175, "name": "range2" },
                    { "startValue": -175, "endValue": -125, "name": "range3" },
                    { "startValue": -125, "endValue": -75, "name": "range4" },
                    { "startValue": -75, "endValue": 0, "name": "range5" }
                ]
    		}
    	},
		"QRCodeBarcode": {
    		"id": "QRCodeBarcode",
            "dependsOn": ["core", "lob", "dv"],
    		"toolboxTitle": "Barcode",
    		"icon": "barcode.png",
    		"dragDropIcon": "barcode-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"width": 150,
			"height": 150,
    		"defaultOptions": {
                 "data": "http://www.infragistics.com/products/jquery/samples"
