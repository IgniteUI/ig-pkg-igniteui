{
    "name": "igniteui",
    "toolboxTitle": "Ignite UI",
    "scripts": [
        "js/jquery-ui.js",
        "js/infragistics.core.js",
        "js/infragistics.lob.js",
        "js/infragistics.dv.js"
    ],
    "stylesheets": [
        "css/jquery-ui.css",
        "css/themes/infragistics/infragistics.theme.css",
        "css/structure/infragistics.css"
    ],
    "iconsPath": "icons",
    "configPath": "config",
    "pluginsPath": "plugins",
    "autocompletePath": "autocomplete", 
    "defaultComponentPlugin": "_default-component-plugin",
    "codeTemplatesPath": "templates",
    "requiresInitialization": true,
    "components": {
    	"grid": {
    		"id": "grid",
    		"toolboxTitle": "Grid",
    		"componentPlugin": "grid-component-plugin", 
    		"icon": "grid.png",
    		"dragDropIcon": "grid-dragdrop.png",
    		"category": "lists",
    		"visual": true, 
    		"framework": false,
    		"height": 300,
    		"width": 300,
    		"dataSource": "sampleData1"
    	},
    	"hierarchicalGrid": {
    		"id": "hierarchicalGrid",
    		"toolboxTitle": "Hierarchical Grid",
    		"icon": "hierarchicalgrid.png",
			"componentPlugin": "hierarchical-grid-component-plugin",
    		"dragDropIcon": "hierarchicalgrid-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 300,
    		"height": 300,
    		"dataSource": "sampleHierarchicalData1",
    		"defaultOptions": {"autoGenerateLayouts": true}
    	},
    	"dataChart": {
    		"id": "dataChart",
    		"toolboxTitle": "Chart",
    		"icon": "chart.png",
    		"dragDropIcon": "chart-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 300,
    		"width": 300,
    		"dataSource": "sampleChartData1"
    	},
		"pieChart": {
    		"id": "pieChart",
    		"toolboxTitle": "Pie Chart",
    		"icon": "pie-chart.png",
    		"dragDropIcon": "pie-chart-dragdrop.png",
			"componentPlugin": "piechart-component-plugin", 
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 300,
    		"width": 300,
			"dataSource": "samplePieChartData",
			"defaultOptions": {"valueMemberPath": "Pop2008", "labelMemberPath": "CountryName", "labelsPosition": "bestFit" } 
		},	
		"sparkline": {
    		"id": "sparkline",
    		"toolboxTitle": "Sparkline",
    		"icon": "sparkline.png",
    		"dragDropIcon": "sparkline-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 100,
    		"width": 300,
			"dataSource": "sampleSparklineData",
			"defaultOptions": {"valueMemberPath": "ExtendedPrice" } 
		},	
		"bulletgraph": {
    		"id": "bulletgraph",
    		"toolboxTitle": "Bulletgraph",
    		"icon": "bullet-graph.png",
    		"dragDropIcon": "bullet-graph-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 100,
    		"width": 300,
			"defaultOptions": { 
				"minimumValue": 0, 
                "maximumValue": 25,
                "interval": 5,
                "value": 17,
                "targetValue": 21,
                "ranges": [
                    {
                        "name": "'bad'",
                        "startValue": 0,
                        "endValue": 15
                    },
                    {
                        "name": "'acceptable'",
                        "startValue": 15,
                        "endValue": 20
                    },
                    {
                        "name": "'good'",
                        "startValue": 20,
                        "endValue": 25
                    }]
			}
		},
		"doughnutChart": {
    		"id": "doughnutChart",
    		"toolboxTitle": "Doughnut Chart",
    		"icon": "doughnut-chart.png",
    		"dragDropIcon": "doughnut-chart-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 500,
    		"width": 500,
			
			"defaultOptions": {
				"series":
					[{
						"name": "Pop1990",
						"labelMemberPath": "CountryName",
						"valueMemberPath": "Pop1990",
						"dataSource": "sampleDoughnutChartData",
						"labelsPosition": "bestFit"
					}]
			} 
		},
		"funnelChart": {
    		"id": "funnelChart",
    		"toolboxTitle": "Funnel Chart",
    		"icon": "funnel-chart.png",
    		"dragDropIcon": "funnel-chart-dragdrop.png",			
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"height": 450,
    		"width": 325,
			"dataSource": "sampleFunnelChartData",
			"defaultOptions": {"valueMemberPath": "Budget", "innerLabelMemberPath": "Budget", "innerLabelVisibility": "visible", "outerLabelMemberPath": "Department",
                "outerLabelVisibility": "visible" } 
		},			
    	"combo": {
    		"id": "combo",
    		"toolboxTitle": "Combo",
    		"icon": "combo.png",
    		"dragDropIcon": "combo-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 200,
    		"height":30,
    		"defaultOptions": {"dropDownAsChild": true},
    		"dataSource": "sampleListData1"
    	},
    	"dataSource": {
    		"id": "dataSource",
    		"toolboxTitle": "Data Source",
    		"icon": "datasource.png",
    		"classId": "$.ig.IgDataSource",
    		"category": "datasources",
    		"visual": false,
    		"framework": true
    	},
    	"dialog": {
    		"id": "dialog",
    		"toolboxTitle": "Dialog Window",
    		"icon": "dialogwindow.png",
    		"dragDropIcon": "dialogwindow-dragdrop.png",
    		"category": "layout",
    		"visual": true,
    		"framework": false,
    		"width": 400,
    		"height": 400
    	},
    	"editor": {
    		"id": "editor",
    		"toolboxTitle": "Editor",
    		"icon": "editor.png",
    		"dragDropIcon": "editor-dragdrop.png",
    		"category": "editors",
    		"visual": true,
    		"framework": false,
    		"width": 150,
    		"height": 30
    	},
    	"fileUpload": {
    		"id": "fileUpload",
    		"toolboxTitle": "File Upload",
    		"icon": "fileupload.png",
    		"dragDropIcon": "fileupload-dragdrop.png",
    		"category": "editors",
    		"visual": true,
    		"width":150,
    		"height":40,
    		"framework": false
    	},
    	"gauge": {
    		"id": "gauge",
    		"toolboxTitle": "Gauge",
    		"icon": "gauge.png",
    		"dragDropIcon": "gauge-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"width": 300,
    		"height": 300,
    		"defaultOptions": {
    			"interval": 2,
                "labelInterval": 1,
                "value": 0,
                "centerX": 0.5,
                "centerY": 0.5,
                "minimumValue": 0,
                "maximumValue": 10,
                "backingOutline": "black",
                "backingStrokeThickness": 1,
                "scaleBrush": "lightblue",
                "scaleStartAngle": 120,
                "scaleEndAngle": 50,
                "scaleStartExtent": 0.5,
                "scaleEndExtent": 0.81,
                "scaleOversweep": 0,
                "tickStartExtent": 0.62,
                "tickEndExtent": 0.69,
                "tickBrush": "black",
                "minorTickBrush": "grey",
                "minorTickStartExtent": 0.65,
                "minorTickEndExtent": 0.69,
                "labelExtent": 0.75,
                "fontBrush": "black",
                "needleShape": "triangle",
                "needleBrush": "red",
                "needleOutline": "maroon",
                "needleStartExtent": -0.2,
                "needleEndExtent": 0.7,
                "needlePivotShape": "none",
                "transitionDuration": 1000,
                "scaleSweepDirection": "clockwise"
    		}
    	},
    	"htmlEditor": {
    		"id": "htmlEditor",
    		"toolboxTitle": "HTML Editor",
    		"icon": "htmleditor.png",
    		"dragDropIcon": "htmleditor-dragdrop.png",
    		"category": "editors",
    		"visual": true,
    		"framework": false,
    		"width": 500,
    		"height": 300
    	},
    	"layoutManager": {
    		"id": "layoutManager",
    		"toolboxTitle": "Layout Manager",
    		"icon": "layoutmanager.png",
    		"dragDropIcon": "layoutmanager-dragdrop.png",
    		"category": "layout",
    		"visual": true,
    		"width": "100%",
    		"height": "100%",
    		"framework": false
    	},
    	"map": {
    		"id": "map",
    		"toolboxTitle": "Map",
    		"icon": "map.png",
    		"dragDropIcon": "map-dragdrop.png",
    		"category": "dataviz",
    		"visual": true,
    		"framework": false,
    		"width": 300,
    		"height": 300
    	},
    	"pivotGrid": {
    		"id": "pivotGrid",
    		"toolboxTitle": "Pivot Grid",
    		"icon": "pivotgrid.png",
    		"dragDropIcon": "pivotgrid-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 400,
    		"height": 400,
    		"dataSource": "samplePivotData1"
    	},
    	"rating": {
    		"id": "rating",
    		"toolboxTitle": "Rating",
    		"icon": "rating.png",
    		"dragDropIcon": "rating-dragdrop.png",
    		"category": "editors",
    		"visual": true,
    		"framework": false,
    		"width": 200,
    		"height":40
    	},
		"datePicker": {
    		"id": "datePicker",
    		"toolboxTitle": "Date Picker",
    		"icon": "datepicker.png",
    		"dragDropIcon": "datepicker-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 200,
    		"height":30
    	},
    	"splitter": {
    		"id": "splitter",
    		"toolboxTitle": "Splitter",
    		"icon": "splitter.png",
    		"dragDropIcon": "splitter-dragdrop.png",
    		"category": "layout",
    		"visual": true,
    		"framework": false
    	},
    	"tileManager": {
    		"id": "tileManager",
    		"toolboxTitle": "Tile Manager",
    		"icon": "tilemanager.png",
    		"dragDropIcon": "tilemanager-dragdrop.png",
    		"category": "layout",
    		"visual": true,
    		"framework": false
    	},
    	"tree": {
    		"id": "tree",
    		"toolboxTitle": "Tree",
    		"icon": "tree.png",
    		"dragDropIcon": "tree-dragdrop.png",
    		"category": "lists",
    		"visual": true,
    		"framework": false,
    		"width": 300,
    		"height": 400,
    		"dataSource": "sampleTreeData1"
    	}
    }
}